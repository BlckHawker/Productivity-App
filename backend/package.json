{
  "name": "backend",
  "version": "1.0.0",
  "main": "server.ts",
  "type": "module",
  "scripts": {
    "dev": "npm run prisma:generate && node --loader ts-node/esm src/server.ts",
    "build": "npm run prisma:generate && tsc",
    "start": "node dist/server.js",
    "expand-env": "node --loader ts-node/esm scripts/expand-env.ts",
    "migrate:dev": "prisma migrate dev --name init",
    "migrate:prod": "prisma migrate deploy",
    "prisma:generate": "prisma generate",
    "test": "jest --passWithNoTests",
    "test:coverage": "jest --coverage --coverageThreshold='{\"global\":{\"branches\":90,\"functions\":90,\"lines\":90,\"statements\":90}}'"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@prisma/client": "^6.13.0",
    "@types/express": "^5.0.3",
    "@types/node": "^24.2.1",
    "axios": "^1.11.0",
    "cors": "^2.8.5",
    "dotenv-expand": "^12.0.2",
    "express": "^5.1.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.5",
    "prisma": "^6.13.0"
  },
  "devDependencies": {
    "@types/axios": "^0.9.36",
    "@types/cors": "^2.8.19",
    "@types/jest": "^30.0.0",
    "jest": "^30.0.5",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.2"
  }
}
